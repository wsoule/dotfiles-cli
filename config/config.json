{
  "personal": {
    "name": "Wyat Soule",
    "email": "wsoule679@gmail.com",
    "editor": "nvim"
  },
  "installation": {
    "homebrew": true,
    "brewfile": true,
    "dotfiles": true,
    "macos_defaults": true,
    "npm_packages": true
  },
  "system": {
    "appearance": {
      "dark_mode": true,
      "enable_24_hour_time": true
    },
    "dock": {
      "autohide": true,
      "position": "bottom",
      "tile_size": 50,
      "magnification": false
    },
    "finder": {
      "show_hidden_files": false,
      "default_view": "column",
      "show_file_extensions": true
    },
    "keyboard": {
      "key_repeat_rate": 2,
      "disable_press_and_hold": true
    },
    "security": {
      "require_password_immediately": true,
      "display_sleep_minutes": 10,
      "computer_sleep_minutes": 30
    },
    "screenshots": {
      "location": "$HOME/Documents/Screenshots",
      "format": "png"
    },
    "menu_bar": {
      "show_battery_percent": true,
      "show_date": true
    },
    "safari": {
      "show_develop_menu": false,
      "default_encoding": "UTF-8"
    }
  },
  "development": {
    "git": {
      "default_branch": "main",
      "pull_rebase": true,
      "push_default": "simple"
    },
    "languages": {
      "node": true,
      "python": false,
      "docker": true,
      "go": false,
      "rust": false,
      "java": false,
      "javascript": false,
      "typescript": false,
      "php": false,
      "ruby": false,
      "csharp": false,
      "cpp": false,
      "swift": false,
      "kotlin": false
    },
    "shell": {
      "theme": "powerlevel10k",
      "terminal_theme": "dark",
      "plugins": {
        "autosuggestions": true,
        "syntax_highlighting": true,
        "fzf": true,
        "zoxide": true
      }
    },
    "aliases": {
      "node": true,
      "git": true,
      "docker": true,
      "system": true
    }
  },
  "packages": {
    "extra_brews": [],
    "extra_casks": [],
    "extra_taps": [],
    "npm_globals": [
      "nx"
    ]
  },
  "directories": [
    "$HOME/Projects",
    "$HOME/Projects/personal",
    "$HOME/Projects/work",
    "$HOME/Projects/playground",
    "$HOME/Scripts",
    "$HOME/Documents/Screenshots"
  ],
  "stow_exclusions": [
    "scripts",
    "private",
    ".git",
    "node_modules"
  ],
  "metadata": {
    "version": "1.0.0",
    "created_at": "2025-09-28T21:15:43Z",
    "last_modified": "2025-09-28T21:44:11Z",
    "created_by": "setup-wizard"
  }
}
